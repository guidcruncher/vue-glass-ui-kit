  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.10.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "diff": "^8.0.2",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "^10.6.2",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.7.4",
    "sass": "^1.95.1",
    "typescript": "~5.9.0",
    "vite": "^7.2.7",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.8"
