    "lint": "eslint . --fix --cache",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "leaflet": "^1.9.4",
    "pinia": "^3.0.3",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
