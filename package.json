{
  "name": "vue-glass-ui-kit",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "prettier --write src/ && vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite pr  eview",
    "build-only": "vite build",
    "sass": "sass ./src/styles/styles-full.scss",
    "mig-styles": "node ./scripts/styleExtractor.js",
    "autorefactor": "node ./scripts/autoRefactor.ts",
